{ 
  "compilerOptions": {
    "target": "ES2015",
    "moduleResolution": "node",
    "sourceMap": true,
    "strict": false,
    "outDir": "js",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false,
    "suppressImplicitAnyIndexErrors": true,
    "esModuleInterop": true, 
  },
  "includes":["ts/**/*"],
  "exclude": [
    "node_modules",
    "typings"
  ]
}